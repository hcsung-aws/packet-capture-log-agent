{
  "protocol": {
    "name": "MMORPG Simulator Protocol",
    "version": "1.0",
    "endian": "little",
    "pack": 1,
    "header": {
      "size": 4,
      "fields": [
        {"name": "length", "type": "uint16", "offset": 0},
        {"name": "type", "type": "uint16", "offset": 2}
      ]
    }
  },
  "types": {
    "CharListEntry": {
      "kind": "struct",
      "fields": [
        {"name": "charUid", "type": "uint64"},
        {"name": "name", "type": "string", "length": 20},
        {"name": "charType", "type": "uint8"},
        {"name": "level", "type": "uint16"}
      ]
    }
  },
  "packets": [
    {
      "type": 257, "name": "CS_LOGIN", "direction": "C2S",
      "fields": [
        {"name": "accountId", "type": "string", "length": 32},
        {"name": "password", "type": "string", "length": 32}
      ]
    },
    {
      "type": 258, "name": "SC_LOGIN_RESULT", "direction": "S2C",
      "fields": [
        {"name": "success", "type": "uint8"},
        {"name": "accountUid", "type": "uint64"},
        {"name": "message", "type": "string", "length": 64}
      ]
    },
    {
      "type": 513, "name": "CS_CHAR_LIST", "direction": "C2S",
      "fields": []
    },
    {
      "type": 514, "name": "SC_CHAR_LIST", "direction": "S2C",
      "fields": [
        {"name": "count", "type": "uint8"},
        {"name": "chars", "type": "array", "length": 5, "element": "CharListEntry"}
      ]
    },
    {
      "type": 515, "name": "CS_CHAR_CREATE", "direction": "C2S",
      "fields": [
        {"name": "charUid", "type": "uint64"},
        {"name": "name", "type": "string", "length": 20},
        {"name": "charType", "type": "uint8"}
      ]
    },
    {
      "type": 516, "name": "SC_CHAR_CREATE_RESULT", "direction": "S2C",
      "fields": [
        {"name": "success", "type": "uint8"},
        {"name": "charUid", "type": "uint64"},
        {"name": "message", "type": "string", "length": 32}
      ]
    },
    {
      "type": 517, "name": "CS_CHAR_SELECT", "direction": "C2S",
      "fields": [
        {"name": "charUid", "type": "uint64"}
      ]
    },
    {
      "type": 518, "name": "SC_CHAR_INFO", "direction": "S2C",
      "fields": [
        {"name": "charUid", "type": "uint64"},
        {"name": "name", "type": "string", "length": 32},
        {"name": "level", "type": "uint16"},
        {"name": "exp", "type": "uint32"},
        {"name": "posX", "type": "int16"},
        {"name": "posY", "type": "int16"},
        {"name": "hp", "type": "uint16"},
        {"name": "maxHp", "type": "uint16"},
        {"name": "gold", "type": "uint64"}
      ]
    },
    {
      "type": 519, "name": "SC_CHAR_LEAVE", "direction": "S2C",
      "fields": [
        {"name": "charUid", "type": "uint64"}
      ]
    },
    {
      "type": 769, "name": "CS_ATTENDANCE_CHECK", "direction": "C2S",
      "fields": []
    },
    {
      "type": 770, "name": "SC_ATTENDANCE_INFO", "direction": "S2C",
      "fields": [
        {"name": "todayAttended", "type": "uint8"},
        {"name": "rewardGold", "type": "uint32"}
      ]
    },
    {
      "type": 771, "name": "SC_ATTENDANCE_RESULT", "direction": "S2C",
      "fields": [
        {"name": "success", "type": "uint8"},
        {"name": "rewardGold", "type": "uint32"}
      ]
    },
    {
      "type": 1025, "name": "CS_MOVE", "direction": "C2S",
      "fields": [
        {"name": "dirX", "type": "int8"},
        {"name": "dirY", "type": "int8"}
      ]
    },
    {
      "type": 1026, "name": "SC_MOVE_RESULT", "direction": "S2C",
      "fields": [
        {"name": "success", "type": "uint8"},
        {"name": "posX", "type": "int16"},
        {"name": "posY", "type": "int16"}
      ]
    },
    {
      "type": 1281, "name": "CS_ATTACK", "direction": "C2S",
      "fields": [
        {"name": "targetUid", "type": "uint64"}
      ]
    },
    {
      "type": 1282, "name": "SC_ATTACK_RESULT", "direction": "S2C",
      "fields": [
        {"name": "success", "type": "uint8"},
        {"name": "targetUid", "type": "uint64"},
        {"name": "damage", "type": "uint16"},
        {"name": "targetHp", "type": "uint16"}
      ]
    },
    {
      "type": 1283, "name": "SC_NPC_SPAWN", "direction": "S2C",
      "fields": [
        {"name": "npcUid", "type": "uint64"},
        {"name": "posX", "type": "int16"},
        {"name": "posY", "type": "int16"},
        {"name": "hp", "type": "uint16"},
        {"name": "maxHp", "type": "uint16"}
      ]
    },
    {
      "type": 1284, "name": "SC_NPC_DEATH", "direction": "S2C",
      "fields": [
        {"name": "npcUid", "type": "uint64"},
        {"name": "expReward", "type": "uint32"}
      ]
    },
    {
      "type": 1285, "name": "SC_EXP_UPDATE", "direction": "S2C",
      "fields": [
        {"name": "exp", "type": "uint32"},
        {"name": "maxExp", "type": "uint32"}
      ]
    },
    {
      "type": 1286, "name": "SC_LEVEL_UP", "direction": "S2C",
      "fields": [
        {"name": "level", "type": "uint16"},
        {"name": "hp", "type": "uint16"},
        {"name": "maxHp", "type": "uint16"},
        {"name": "atk", "type": "uint16"},
        {"name": "def", "type": "uint16"}
      ]
    },
    {
      "type": 65281, "name": "CS_HEARTBEAT", "direction": "C2S",
      "fields": []
    },
    {
      "type": 65282, "name": "SC_HEARTBEAT", "direction": "S2C",
      "fields": []
    },
    {
      "type": 65283, "name": "SC_ERROR", "direction": "S2C",
      "fields": [
        {"name": "errorCode", "type": "uint16"},
        {"name": "message", "type": "string", "length": 64}
      ]
    }
  ]
}
